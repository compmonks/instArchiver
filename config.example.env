# Copy to .env and replace the placeholder values. Do not commit real tokens.

# Required: Instagram Business/User ID for the professional account.
IG_USER_ID=1234567890

# Required: long-lived user access token obtained via the Instagram Graph API.
IG_ACCESS_TOKEN=EAAGXXXXXXXX

# Optional: override the Graph API version (default: v19.0)
IG_API_VERSION=v19.0

# Optional: output directory for the archive (default: InstagramArchive)
IG_ARCHIVE_DIR=InstagramArchive
